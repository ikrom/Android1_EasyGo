<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tc.easygo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashAcitivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".TipsActivity"></activity>
        <activity android:name=".PlanActivity"></activity>
        <activity android:name=".DetailTipsActivity"></activity>
        <activity android:name=".PlanCreateActivity"></activity>
        <activity android:name=".ProfilEditTag"></activity>
        <activity android:name=".ProfilOtherActivity"></activity>
        <activity android:name=".PopularDestinationActivity"></activity>
        <activity
            android:name=".TipsItemActivity"
            android:label="@string/title_activity_tips_item"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".TipsItemDetail"
            android:label="@string/title_activity_tips_item_detail"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".PopularDestinationDetailActivity"
            android:label="@string/title_activity_popular_destination_detail"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".GalleryActivity"></activity>
        <activity
            android:name=".DetailImageActivity"
            android:label="@string/title_activity_detail_image"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".StaggeredGridActivity"></activity>
        <activity
            android:name=".CheckPreferenceActivity"
            android:label="@string/title_activity_check_preference"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".JelajahActivity"
            android:label="@string/title_activity_jelajah"
            android:theme="@style/AppTheme" >
        </activity>
    </application>

</manifest>
